{
  "web": {
    "host": "0.0.0.0",
    "listen": 8080,
    "ssl": {
      "enabled": false,
      "certificate": "",
      "key": ""
    }
  },
  "docker": {
    "network": {
      "name": "controlpanel_nw"
    },
    "socket": "/var/run/docker.sock",
    "autoupdate_images": true,
    "timezone_path": "/etc/timezone",
    "interface": "172.18.0.1"
  },
  "filesystem": {
    "server_logs": "/tmp/controlpanel"
  },
  "sftp": {
    "path": "/srv/daemon-data",
    "ip": "0.0.0.0",
    "port": 2022,
    "keypair": {
      "bits": 2048,
      "e": 65537
    }
  },
  "remote": {
    "base": "https://panel.bluefoxhost.com"
  },
  "uploads": {
    "size_limit": 1024
  },
  "livechat": {
    "enable": false,
    "_COMMENT FOR DEVS_": "1 = livechatinc.com, 2 = https://chatra.com, 3 = https://www.tawk.to, 4 = https://crisp.chat",
    "livechatv": 1, 
    "api": "API-KEY-HERE"
  }
}
